{
    "name": "flame05/flarum-mediaembed",
    "type": "flarum-extension",
    "description": "Embed third-party content in your Flarum. Code of s9e.",
    "homepage": "http://s9etextformatter.readthedocs.org/Plugins/MediaEmbed/Sites/",
    "keywords": ["embed","facebook","media","twitter","youtube"],
    "version": "v0.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bernasconi - Using s9e code - https://github.com/s9e",
            "email": "daniel@cerchio.ch",
            "homepage": "http://daniel.world"
        }
    ],
    "support": {
        "issues": "https://github.com/flame05/flarum-mediaembed/issues",
        "source": "https://github.com/flame05/flarum-mediaembed"
    },
    "require": {
        "php": ">=5.4.0",
        "flarum/core": "^0.1.0-beta.3"
    },
    "autoload": {
        "psr-4": {
            "Flarum\\MediaEmbed\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "MediaEmbed",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        }
    }
}
